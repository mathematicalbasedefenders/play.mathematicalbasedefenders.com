    "adaptive-scale": "^1.0.0",
    "ajv": "^8.12.0",
    "async_hooks": "^1.0.0",
    "bcrypt": "^5.1.0",
    "bufferutil": "^4.0.7",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dompurify": "^3.0.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.1",
    "fast-json-stringify": "^5.7.0",
    "fontfaceobserver": "^2.3.0",
    "helmet": "^7.0.0",
    "howler": "^2.2.4",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.7.0",
    "jsdom": "^22.1.0",

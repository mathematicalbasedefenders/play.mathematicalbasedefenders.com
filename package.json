{
  "name": "play.mathematicalbasedefenders.com",
  "version": "0.4.13",
  "private": true,
  "description": "Fun math game",
  "main": "server.js",
  "scripts": {
    "test-jest": "jest",
    "test-cypress": "cypress run",
    "test-cypress-headed": "cypress run --headed",
    "build": "webpack --config ./webpack.config.production.js",
    "build-dev": "webpack --config ./webpack.config.development.js",
    "compile": "tsc",
    "copyStaticFiles": "copyfiles --up 1 \"src/**/*\" -e \"**/*.ts\" dist",
    "client-start": "cd client && npm run compile && npm run build && npm run copyStaticFiles && node ./dist/index.js",
    "client-start-dev": "cd client && npm run compile && npm run build-dev && npm run copyStaticFiles && node ./dist/index.js",
    "server-start": "cd server && npm run compile && npm run copyStaticFiles && node ./dist/index.js",
    "server-start-dev": "cd server && npm run compile && npm run copyStaticFiles && node ./dist/index.js",
    "start-dev": "concurrently --kill-others \"npm run client-start-dev\" \"npm run server-start-dev\"",
    "start-concurrent": "cross-env NODE_ENV=production concurrently --kill-others \"npm run server-start\" \"npm run client-start\"",
    "fix-node-version": "nvm use"
  },
  "overrides": {
    "semver": "^7.5.2",
    "tough-cookie": "^4.1.3",
    "@types/css-font-loading-module": "^0.0.8"
  },
  "author": "mistertfy64",
  "license": "AGPL-3.0-or-later",

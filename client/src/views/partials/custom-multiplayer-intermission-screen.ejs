<div id="main-content__custom-multiplayer-intermission-screen-container">
  <button class="custom-multiplayer-screen__sidebar-item" title="Go back to the main menu." id="custom-multiplayer-screen__sidebar-item--back">Back</button>
  <div id="main-content__custom-multiplayer-intermission-screen-container__left">
    <div id="main-content__custom-multiplayer-intermission-screen-container__status" class="text--light">
      <div class="main-content__custom-multiplayer-intermission-screen-container__status-indicator">
        <span class="status-indicator-label">Status</span>
        <span id="main-content__custom-multiplayer-intermission-screen-container__game-status-message" class="text--light">Custom Multiplayer Intermission Screen</span>
      </div>
      <div class="main-content__custom-multiplayer-intermission-screen-container__status-indicator">
        <span class="status-indicator-label">Previous Game</span>
        <div id="main-content__custom-multiplayer-intermission-screen-container__game-status-ranking" class="text--light"></div>
      </div>
    </div>
  </div>
  <div id="main-content__custom-multiplayer-intermission-screen-container__center"></div>
  <div id="main-content__custom-multiplayer-intermission-screen-container__right">
    <!-- <span id="main-content__custom-multiplayer-intermission-screen-container__player-list" class="text--light"></span>
    <br> -->
    <div id="main-content__custom-multiplayer-intermission-screen-container__chat" class="text--light">
      <div id="main-content__custom-multiplayer-intermission-screen-container__chat__messages"></div>
      <div id="main-content__custom-multiplayer-intermission-screen-container__chat__player-list"></div>
      <div id="main-content__custom-multiplayer-intermission-screen-container__chat__entry">
        <input autocomplete="off" maxlength="256" id="chat-message" type="text"><button id="message-send-button">Send</button>
      </div>
      <div id="main-content__custom-multiplayer-intermission-screen-container__player-list">
        <span>
          In Room: <span id="main-content__custom-multiplayer-intermission-screen-container__player-count">0</span>
        </span>
        <span id="main-content__custom-multiplayer-intermission-screen-container__player-list__toggle-list">
          Show Player List
        </span>
      </div>
    </div>
  </div>

</div>
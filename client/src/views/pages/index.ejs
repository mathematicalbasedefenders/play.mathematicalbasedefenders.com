  </div>

  <%- include("../partials/user-card") %>
  <%- include("../partials/status-tray") %>
  <%- include("../partials/chat-tray") %>
  <%- include("../partials/on-screen-keyboard") %>
  <%- include("../partials/toast-notification") %>
  <%- include("../partials/popup-notification") %>
  <%- include("../partials/version-number") %>

</body>

<% if (optimizeForProduction) { %>
<%- include("../partials/production-scripts.ejs") %>
<% } else { %>
<%- include("../partials/development-scripts.ejs") %>

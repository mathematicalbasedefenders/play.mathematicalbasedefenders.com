<body>

  <div id="opening-screen-container">
    <%- include("../partials/opening-screen") %>
  </div>

  <div id="main-content-container">
    <%- include("../partials/main-menu-screen") %>
    <%- include("../partials/singleplayer-menu-screen") %>
